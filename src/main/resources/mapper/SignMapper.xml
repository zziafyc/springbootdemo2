<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhongyong.springbootdemo2.mapper.SignMapper">
    <insert id="insertSign" parameterType="com.zhongyong.springbootdemo2.model.Sign">
        INSERT INTO sign(staff_id,group_id,sign_time) VALUES (#{staffId},#{groupId},#{signTime})
    </insert>
</mapper>